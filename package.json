{
    "$schema": "./node_modules/ng-packagr/package.schema.json",
    "ngPackage": {
        "lib": {
            "entryFile": "src/sui.module.ts"
        }
    },
    "name": "ng2-semantic-ui",
    "main": "dist/bundles/ng2-semantic-ui.umd.js",
    "module": "dist/fesm5/ng2-semantic-ui.js",
    "typings": "dist/ng2-semantic-ui.d.ts",
    "version": "0.9.7",
    "description": "Angular 2 Semantic UI Components",
    "repository": {
        "type": "git",
        "url": "https://github.com/edcarroll/ng2-semantic-ui/tree/master"
    },
    "bugs": {
        "url": "https://github.com/edcarroll/ng2-semantic-ui/tree/master/issues"
    },
    "author": "Edward Carroll",
    "keywords": [
        "angular",
        "angular-components",
        "semantic-ui",
        "no-jquery",
        "ui",
        "typescript"
    ],
    "license": "MIT",
    "angular-cli": {},
    "scripts": {
        "run:demo": "ng serve",
        "lint:lib": "tslint -c tslint.json --project src/tsconfig.json",
        "lint:demo": "tslint -c tslint.json --project demo/src/tsconfig.json",
        "lint": "npm run lint:lib & npm run lint:demo",
        "compile:lib": "node ./scripts/clean && tsc -p src/tsconfig.json && ngc -p src/tsconfig.json",
        "compile:lib:w": "tsc -p src/tsconfig.json -w",
        "compile:demo": "ng build --prod --base-href=/ng2-semantic-ui/",
        "compile": "npm run compile:lib && npm run compile:demo",
        "package:lib": "node ./scripts/bundle && node ./scripts/locales",
        "locales": "node ./scripts/locales",
        "build:lib": "npm run lint:lib && ng-packagr -p package.json",
        "build:demo": "npm run lint && npm run compile",
        "build": "npm run lint && npm run compile && npm run package:lib",
        "prepublishOnly": "npm run build",
        "test": "ng test",
        "test:cov": "ng test --code-coverage true",
        "test:w": "ng test --single-run false --browsers Chrome --reporters kjhtml"
    },
    "peerDependencies": {
        "bowser": "^1.7.2",
        "date-fns": "2.0.0-alpha.1",
        "rxjs": "^6.5.3",
        "rxjs-compat": "^6.5.3",
        "popper.js": "^1.14.0",
        "element-closest": "^2.0.2",
        "extend": "^3.0.1",
        "is-ua-webview": "^1.0.0",
        "tslib": "^1.10.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.16",
        "@angular/cli": "^8.3.16",
        "@angular/common": "^8.2.13",
        "@angular/compiler": "^8.2.13",
        "@angular/compiler-cli": "^8.2.13",
        "@angular/core": "^8.2.13",
        "@angular/forms": "^8.2.13",
        "@angular/platform-browser": "^8.2.13",
        "@angular/platform-browser-dynamic": "^8.2.13",
        "@angular/router": "^8.2.13",
        "@edcarroll/code-style": "^2.0.0",
        "@types/extend": "^3.0.0",
        "@types/jasmine": "^2.5.53",
        "@types/prismjs": "~1.4.18",
        "bowser": "^1.7.2",
        "core-js": "^2.5.3",
        "date-fns": "2.0.0-alpha.1",
        "element-closest": "^2.0.2",
        "extend": "^3.0.1",
        "fs-extra": "^5.0.0",
        "is-ua-webview": "^1.0.0",
        "jasmine-core": "~2.5.2",
        "jasmine-spec-reporter": "~3.2.0",
        "karma": "~1.4.0",
        "karma-chrome-launcher": "~2.0.0",
        "karma-coverage-istanbul-reporter": "^1.3.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-min-reporter": "^0.1.0",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-remap-istanbul": "~0.4.0",
        "ng-packagr": "^5.7.1",
        "popper.js": "^1.14.0",
        "prismjs": "~1.6.0",
        "protractor": "~5.0.0",
        "rollup": "~0.41.4",
        "rollup-analyzer": "^1.0.2",
        "rollup-plugin-commonjs": "~7.0.0",
        "rollup-plugin-node-resolve": "~2.0.0",
        "rollup-plugin-uglify": "~1.0.1",
        "rxjs": "^6.5.3",
        "rxjs-compat": "^6.5.3",
        "rxjs-tslint": "^0.1.7",
        "ts-node": "^5.0.1",
        "tslib": "^1.10.0",
        "tslint": "^5.20.1",
        "typescript": "3.5.3",
        "web-animations-js": "^2.3.2",
        "zone.js": "~0.9.1"
    }
}
